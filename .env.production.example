# TRYONYOU Production Environment Variables
# Copy this file to .env.production and fill in your actual values
# IMPORTANT: Never commit actual credentials to version control

# Vercel Deployment Token
VITE_VERCEL_TOKEN=your_vercel_token_here

# Google API Key for AI services
VITE_GOOGLE_API_KEY=your_google_api_key_here

# Environment Configuration
VITE_PILOT_MODE=LAFAYETTE_ACTIVE
VITE_ENVIRONMENT=production

# Porkbun DNS API Key
PORKBUN_API_KEY=your_porkbun_api_key_here

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
