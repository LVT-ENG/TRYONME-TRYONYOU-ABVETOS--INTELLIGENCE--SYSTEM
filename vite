@app.post("/api/scan")
async def process_scan(data: dict):
    # Simulaci√≥n de recepci√≥n de esc√°ner corporal
    h = data.get("height")
    w = data.get("weight")
    event = data.get("event")

    # Notificaci√≥n de Agente a Telegram
    token = os.getenv("TELEGRAM_TOKEN")
    chat_id = os.getenv("TELEGRAM_CHAT_ID")
    if token and chat_id:
        msg = f"üíé NUEVO ESCANEO: {h}cm, {w}kg para evento {event}. Fit Accuracy: 99.8%"
        requests.post(f"https://api.telegram.org/bot{token}/sendMessage",
                      json={"chat_id": chat_id, "text": msg})

    return {"recommendation": "Robe de Soir√©e Lafayette", "match": "99.8%"}
