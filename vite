bash-3.2$ cat << 'EOF' > src/pages/Home.jsx
> import React, { useState } from 'react';
> 
> export default function Home() {
>   const [scanning, setScanning] = useState(false);
> 
>   const style = {
>     body: { backgroundColor: '#000', color: '#C5A46D', minHeight: '100vh', display: 'flex', flexDirection: 'column', alignItems: 'center', justifyContent: 'center', fontFamily: 'serif', textAlign: 'center', margin: 0 },
>     line: { width: '100px', height: '1px', backgroundColor: '#C5A46D', margin: '20px auto' },
>     btn: { border: '1px solid #C5A46D', backgroundColor: 'transparent', color: '#C5A46D', padding: '15px 40px', cursor: 'pointer', letterSpacing: '4px', marginTop: '30px', fontSize: '12px' }
>   };
> 
>   if (scanning) {
>     return (
>       <div style={style.body}>
>         <div style={{ width: '200px', height: '350px', border: '1px solid #C5A46D', position: 'relative', overflow: 'hidden' }}>
>             <div style={{ width: '100%', height: '2px', backgroundColor: '#C5A46D', position: 'absolute', top: '0', boxShadow: '0 0 10px #C5A46D', animation: 'scan 3s infinite linear' }}></div>
>         </div>
>         <h2 style={{ letterSpacing: '5px', marginTop: '30px' }}>ANALYSE BIOM√âTRIQUE...</h2>
>         <button onClick={() => setScanning(false)} style={{ color: '#fff', opacity: 0.3, background: 'none', border: 'none', marginTop: '20px', cursor: 'pointer' }}>ANNULER</button>
>         <style>{`@keyframes scan { 0% { top: 0% } 50% { top: 100% } 100% { top: 0% } }`}</style>
>       </div>
>     );
>   }
> 
>   return (
>     <div style={style.body}>
>       <h1 style={{ fontSize: '3rem', letterSpacing: '10px', margin: 0 }}>GALERIES LAFAYETTE</h1>
>       <div style={style.line}></div>
>       <h2 style={{ fontStyle: 'italic', fontWeight: '100' }}>L'Intelligence Sur Mesure</h2>
>       <p style={{ opacity: 0.6, maxWidth: '400px' }}>D√©couvrez la pi√®ce qui vous sublime. Sans chiffres, sans tailles.</p>
>       <button style={style.btn} onClick={() => setScanning(true)}>LANCER L'EXP√âRIENCE</button>
>       <footer style={{ position: 'absolute', bottom: '20px', fontSize: '10px', opacity: 0.3 }}>PROT√âG√â PAR BREVET PCT/EP2025/067317</footer>
>     </div>
>   );
> }
