> export default function Home() {
>   const [step, setStep] = useState(1);
>   const [claimIdx, setClaimIdx] = useState(0);
> 
>   useEffect(() => {
>     const timer = setInterval(() => setClaimIdx(prev => (prev + 1) % CLAIMS.length), 5000);
>     return () => clearInterval(timer);
>   }, []);
> 
>   return (
>     <div className="notranslate" style={{ backgroundColor: '#0a0a0a', color: '#fff', minHeight: '100vh', fontFamily: 'serif', textAlign: 'center', position: 'relative', overflow: 'hidden' }}>
