def notify_telegram(msg):
    token = os.getenv("TELEGRAM_TOKEN")
    chat_id = os.getenv("TELEGRAM_CHAT_ID")
    if token and chat_id:
        try:
            requests.post(f"https://api.telegram.org/bot{token}/sendMessage",
                          json={"chat_id": chat_id, "text": f"üíé JULES V7:\n{msg}"})
        except:
            pass

@app.get("/api/jules")
async def jules_master():
    notify_telegram("‚úÖ Ecosistema Consolidado: Acceso de cliente detectado.")
    return JSONResponse(content={
        "status": "ü§ñ JULES V7 MASTER ACTIVE",
        "business": BUSINESS_CORE,
        "narrative": "Pau le Paon confirmando dignidad y ajuste.",
        "technical": "All Assets Merged Successfully"
    })
EOF
