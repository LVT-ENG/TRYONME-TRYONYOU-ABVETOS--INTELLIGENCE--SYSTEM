import React, { useState } from 'react';

export default function Home() {
  const [view, setView] = useState('landing');

  const s = {
    bg: { backgroundColor: '#0a0a0a', color: '#FFFFFF', minHeight: '100vh', display: 'flex', flexDirection: 'column', alignItems: 'center', justifyContent: 'center', textAlign: 'center', fontFamily: 'serif' },
    gold: '#C5A46D',
    btn: { background: 'white', color: 'black', border: 'none', padding: '20px 40px', cursor: 'pointer', letterSpacing: '4px', fontWeight: 'bold', marginTop: '30px' }
  };

  if (view === 'scanning') {
    return (
      <div style={s.bg}>
        <div style={{ width: '200px', height: '300px', border: '2px solid' + s.gold, position: 'relative' }}>
           <div style={{ width: '100%', height: '2px', background: s.gold, position: 'absolute', top: '50%' }}></div>
        </div>
        <h2 style={{ color: s.gold, marginTop: '20px' }}>ANALYSE BIOM√âTRIQUE...</h2>
        <button onClick={() => setstyle={{ color: 'white', background: 'none', border: 'none', marginTop: '20px', opacity: 0.5 }}>ANNULER</button>
      </div>
    );
  }

  return (
    <div className="notranslate" style={s.bg}>
      <h1 style={{ fontSize: '3rem', letterSpacing: '10px' }}>GALERIES LAFAYETTE</h1>
      <p style={{ fontStyle: 'italic', color: s.gold, fontSize: '1.2rem' }}>L'Intelligence Sur Mesure</p>
      <button onClick={() => setView('scanning')} style={s.btn}>LANCER L'EXP√âRIENCE</button>
      <footer style={{ position: 'absolute', bottom: '20px', fontSize: '10px', opacity: 0.3 }}>
        PROT√âG√â PAR BREVET PCT/EP2025/067317 | LVT-ENG ¬© 2026
      </footer>
    </div>
  );
}
