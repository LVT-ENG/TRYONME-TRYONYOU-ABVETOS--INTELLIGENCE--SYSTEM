import requests

app = FastAPI()

def notify_telegram(message):
    token = os.getenv("TELEGRAM_TOKEN")
    chat_id = os.getenv("TELEGRAM_CHAT_ID")
    if token and chat_id:
        url = f"https://api.telegram.org/bot{token}/sendMessage"
        requests.post(url, json={"chat_id": chat_id, "text": f"ü§ñ JULES V7:\n{message}"})

@app.get("/api/jules")
async def jules_heartbeat():
    notify_telegram("‚úÖ Latido detectado. El Piloto de Lafayette est√° operativo.")
    return JSONResponse(content={
        "status": "ü§ñ JULES V7 ACTIVO",
        "bridge": "Narrativa-T√©cnica Sincronizada",
        "legal": "Protected by Patent PCT/EP2025/067317",
        "mission": "Restore human dignity in fashion"
    })
EOF
