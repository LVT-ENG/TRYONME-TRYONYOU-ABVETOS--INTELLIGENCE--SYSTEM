import React, { useState } from 'react';
import { motion, AnimatePresence } from 'framer-motion';

export default function Home() {
  const [view, setView] = useState('landing');

  return (
    <div style={{ backgroundColor: '#141619', color: '#FFFFFF', minHeight: '100vh', fontFamily: 'serif', overflow: 'hidden', position: 'relative' }}>
      <AnimatePresence mode="wait">
        {view === 'landing' ? (
          <motion.div 
            key="l" initial={{ opacity: 0 }} animate={{ opacity: 1 }} exit={{ opacity: -1 }}
            style={{ display: 'flex', flexDirection: 'column', alignItems: 'center', justifyContent: 'center', height: '100vh', textAlign: 'center' }}
          >
            <h1 style={{ fontSize: '3.5rem', letterSpacing: '10px', fontWeight: '200', margin: '0' }}>GALERIES LAFAYETTE</h1>
            <div style={{ width: '100px', h: '1px', background: '#C5A46D', margin: '20px 0' }} />
            <h2 style={{ fontSize: '1.8rem', fontStyle: 'ital: '100', color: '#C5A46D' }}>L'Intelligence Sur Mesure</h2>
            
            <p style={{ maxWidth: '600px', opacity: 0.7, margin: '30px', lineHeight: '1.8', fontWeight: '200' }}>
              D√©couvrez la pi√®ce qui vous sublime. Pas de chiffres, pas de tailles. 
              Juste votre silhouette magnifi√©e par notre IA Jules V7.
            </p>

            <button 
              onClick={() => setView('scanning')}
              style={{ background: 'transparent', border: '1px solid #C5A46D', color: '#C5A46D', padding: '20px 50px', cursor: 'pointer', letterSpacing: '3px', fontSize: '0.8rem', transition: '0.3s' }}
              onMouseOver={(e) => e.target.style.background = '#C5A46D33'}
              onMouseOut={(e) => e.target.style.background = 'transparent'}
            >
              LANCER L'EXP√âRIENCE
            </button>
            
            <footer style={{ position: 'absolute', bottom: '30px', fontSize: '9px', opacity: 0.4, letterSpacing: '2px' }}>
              PROT√âG√â PAR BREVET PCT/EP2025/067317 | LVT-ENG ¬© 2026
            </footer>
          </motion.div>
        ) : (
          <motion.div 
            key="s" initial={{ opacity: 0 }} animate={{ opacity: 1 }}
            style={{ display: 'flex', flexDirection: 'column', alignItems: 'center', justifyContent: 'center', height: '100vh', background: '#000' }}
          >
            <div style={{ width: '250px', height: '400px', border: '1px solid #C5A46D', borderRadius: '150px 150px 0 0', position: 'relative', overflow: 'hidden' }}>
              <motion.div 
                animate={{ top: ['0%', '100%', '0%'] }} 
                transition={{ duration: 4, repeat: Infinity, ease: "linear" }}
                style={{ width: '100%', height: '2px', background: '#C5A46D', position: 'absolute', boxShadow: '0 0 15px #C5A46D' }} 
              />
            </div>
            <h3 style={{ marginTop: '40px', color: '#C5A46D', letterSpacing: '5px', fontWeight: '200' }} className="animate-pulse">ANALYSE BIOM√âTRIQUE...</h3>
            <button onClick={() => setView('landing')} style={{ marginTop: '20px', color: '#FFF', opacity: 0.3, background: 'none', border: 'none', cursor: 'pointer' }}>Annuler</button>
          </motion.div>
        )}
      </AnimatePresence>
    </div>
  );
}
EOF
cat << 'EOF' > src/pages/Home.jsx
import React, { useState } from 'react';
import { motion, AnimatePresence } from 'framer-motion';

export default function Home() {
  const [view, setView] = useState('landing');

  return (
    <div className="notranslate" style={{ backgroundColor: '#141619', color: '#FFFFFF', minHeight: '100vh', fontFamily: 'serif', overflow: 'hidden', position: 'relative' }}>
      <AnimatePresence mode="wait">
        {view === 'landing' ? (
          <motion.div 
            key="l" initial={{ opacity: 0 }} animate={{ opacity: 1 }} exit={{ opacity: 0 }}
            style={{ display: 'flex', flexDirection: 'column', alignItems: 'center', justifyCenter: 'center', height: '100vh', textAlign: 'center', paddingTop: '15vh' }}
          >
            <h1 style={{ fontSize: 'clamp(2rem, 8vw, 4rem)', letterSpacing: '12px', fontWeight: '200', margin: '0', textTransform: 'uppercase' }}>GALERIES LAFAYETTE</h1>
            <div style={{ width: '60px', height: '1px', background: '#C5A430px auto' }} />
            
            <h2 style={{ fontSize: 'clamp(1.2rem, 4vw, 2.2rem)', fontStyle: 'italic', fontWeight: '100', color: '#C5A46D', marginBottom: '40px' }}>
              L'Intelligence Sur Mesure
            </h2>
            
            <p style={{ maxWidth: '500px', opacity: 0.6, margin: '0 auto 50px', lineHeight: '1.8', fontWeight: '200', padding: '0 20px' }}>
              D√©couvrez la pi√®ce qui vous sublime. Sans chiffres, sans tailles. 
              Votre silhouette magnifi√©e par notre IA Jules V7.
            </p>

            <button 
              onClick={() => setView('scanning')}
              style={{ background: 'transparent', border: '1px solid #C5A46D', color: '#C5A46D', padding: '18px 45px', cursor: 'pointer', letterSpacing: '4px', fontSize: '0.75rem', transition: '0.4s all' }}
              onMouseOver={(e) => {e.target.style.background = '#C5A46D'; e.target.style.color = '#000';}}
              onMouseOut={(e) => {e.target.style.background = 'transparent'; e.target.style.color = '#C5A46D';}}
            >
              LANCER L'EXP√âRIENCE
            </button>
            
            <footer style={{ position: 'absolute', bottom: '40px', width: '100%', fontSize: '9px', opacity: 0.3, letterSpacing: '3px', textTransform: 'uppercase' }}>
              PROT√âG√â PAR BREVET PCT/EP2025/067317 | LVT-ENG ¬© 2026
            </footer>
          </motion.div>
        ) : (
          <motion.div 
            key="s" initial={{ opacity: 0 }} animate={{ opacity: 1 }}
            style={{ display: 'flex', flexDirection: 'column', alignItems: 'center', justifyContent: 'center', height: '100vh', background: '#0d0e10' }}
          >
            <div style={{ width: '220px', height: '380px', border: '1px solid #C5A46D', borderRadius: '110px 110px 0 0', position: 'relative', overflow: 'hidden' }}>
              <motion.div 
                animate={{ top: ['0%', '100%', '0%'] }} 
                transition={{ duration: 3, repeat: Infinity, ease: "linear" }}
                style={{ width: '100%', height: '2px', background: '#C5A46D', position: 'absolute', boxShadow: '0 0 20px #C5A46D' }} 
              />
            </div>
            <h3 style={{ marginTop: '50px', color: '#C5A46D', letterSpacing: '6px', fontWeight: '200', fontSize: '0.9rem' }}>ANALYSE BIOM√âTRIQUE EN COURS</h3>
            <button onClick={() => setView('landing')} style={{ marginTop: '30px', color: '#FFF', opacity: 0.2, background: 'none', border: 'none', cursor: 'pointer', fontSize: '10px', letterSpacing: '2px' }}>ANNULER</button>
          </motion.div>
        )}
      </AnimatePresence>
    </div>
  );
}
