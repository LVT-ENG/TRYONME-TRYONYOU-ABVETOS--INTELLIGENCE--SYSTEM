from fastapi import FastAPI
from fastapi.responses import JSONResponse
import os, requests

app = FastAPI()

# ESTRUCTURA DE VALOR CONSOLIDADA
BUSINESS_CORE = {
    "client": "Galeries Lafayette / Hub71",
    "price_per_pilot": "4,900‚Ç¨ / mes",
    "patent": "PCT/EP2025/067317",
    "target": "Reducci√≥n de devoluciones al 0%",
    "demo_url": "https://tryonyou.app/demo"
}

def notify_telegram(msg):
    token = os.getenv("TELEGRAM_TOKEN")
    chat_id = os.getenv("TELEGRAM_CHAT_ID")
    if token and chat_id:
        requests.post(f"https://api.telegram.org/bot{token}/sendMessage",
                      json={"chat_id": chat_id, "text": f"üíé COHERENCIA V7:\n{msg}"})

