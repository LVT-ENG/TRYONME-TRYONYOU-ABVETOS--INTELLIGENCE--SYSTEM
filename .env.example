# TRYONYOU Environment Variables

# Application Mode
NODE_ENV=production
VITE_ENV=production

# ABVET Biometric Authentication
VITE_ABVET_MODE=demo
VITE_ABVET_API_ENDPOINT=https://api.tryonyou.app/abvet
VITE_ABVET_API_KEY=your_abvet_api_key_here

# FTT (Fashion Trend Tracker) API
VITE_FTT_API_ENDPOINT=https://api.tryonyou.app/trends
VITE_FTT_API_KEY=your_ftt_api_key_here

# CAP (Creative Auto-Production) API
VITE_CAP_API_ENDPOINT=https://api.tryonyou.app/production
VITE_CAP_API_KEY=your_cap_api_key_here

# Telegram Bot (Deploy Express)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Vercel Deployment
VERCEL_TOKEN=your_vercel_token_here
VERCEL_ORG_ID=your_vercel_org_id_here
VERCEL_PROJECT_ID=your_vercel_project_id_here

# Analytics (Optional)
VITE_GA_TRACKING_ID=
VITE_HOTJAR_ID=

# Feature Flags
VITE_ENABLE_PAU_OVERLAY=true
VITE_ENABLE_FTT_CAP_SYNC=false
VITE_ENABLE_ABVET_AUTH=true
VITE_ENABLE_I18N=true
