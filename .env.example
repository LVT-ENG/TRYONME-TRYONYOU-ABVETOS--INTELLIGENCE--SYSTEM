# TRYONYOU Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# Google AI API Key for vision/photo scanning features
# Get your key from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Vercel deployment tokens (optional, for automated deployments)
VERCEL_TOKEN=your_vercel_token_here
VERCEL_ORG_ID=your_vercel_org_id_here
VERCEL_PROJECT_ID=your_vercel_project_id_here

# Telegram notifications (optional, for deployment notifications)
TELEGRAM_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
