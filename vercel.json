{
  "framework": "vite",
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/demo", "destination": "/index.html" },
    { "source": "/checkout", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
