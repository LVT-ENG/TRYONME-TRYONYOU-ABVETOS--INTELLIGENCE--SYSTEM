{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/docs/(.*)",
      "dest": "/docs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/web/index.html"
    }
  ],
  "ignore": ["!docs/**"],
  "env": {
    "NODE_ENV": "production"
  },
  "github": {
    "enabled": true,
    "silent": false,
    "autoJobCancelation": true
  },
  "build": {
    "env": {
      "VITE_ENV": "production"
    }
  }
}